<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="563.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.ViewContactsSceneController">
   <children>
      <Label layoutX="25.0" layoutY="23.0" text="Contacts">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextArea fx:id="contactsViewTextArea" layoutX="25.0" layoutY="64.0" prefHeight="245.0" prefWidth="752.0" />
      <Button layoutX="25.0" layoutY="328.0" mnemonicParsing="false" onAction="#viewContactButtonOnClick" prefHeight="35.0" prefWidth="128.0" text="View Contacts">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="nameTextField" layoutX="602.0" layoutY="381.0" promptText="Enter Name" />
      <TextField fx:id="numberTextField" layoutX="602.0" layoutY="435.0" promptText="Enter Number" />
      <Button layoutX="649.0" layoutY="488.0" mnemonicParsing="false" onAction="#addContactButtonOnClick" prefHeight="35.0" prefWidth="128.0" text="Add Contact">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
